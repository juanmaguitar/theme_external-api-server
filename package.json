{
  "name": "angular-phonecat-custom",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dist-fonts": "mv dist/glyphicons* dist/css",
    "dist": "git add dist && git commit -am \"dist $(date +'%h-%m %s')\" &&  git push",
    "gh-pages": "git subtree push --prefix dist origin gh-pages",
    "deploy": "npm run build && npm run dist && npm run gh-pages",
    "build": "rimraf dist && webpack --bail --progress --profile && npm run dist-fonts",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "start": "npm run server"
  },
  "author": "",
  "license": "ISC",
